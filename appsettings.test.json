{
    "pgDb": {
        "connectionString": "postgres://postgres:postgres@localhost:5432/postgres"
    },
    "mySqlDb": {
        "connectionString": "mysql://root:password@localhost/webapi"
    },
    "access": {
        "user": "test",
        "password": "1234567890",
        "authentication": {
            "test": "1234567890"
        }
    },
    "error": {
        "-1": "Database error"
    },
    "service": {
        "serviceName": "webapi",
        "priority": 0,
        "httpHelth": "http://{host}/helth",
        "mqHelth": "/webapi/helth",
        "command": [
            {
                "objectType": "GetCars",
                "httpTo": "http://{host}/cars",
                "mqTo": "/webapi/request"
            },
            {
                "objectType": "GetCar",
                "httpTo": "http://{host}/car/get",
                "mqTo": "/webapi/request"
            },
            {
                "objectType": "AddCar",
                "httpTo": "http://{host}/car/add",
                "mqTo": "/webapi/request"
            },
            {
                "objectType": "ModifyCar",
                "httpTo": "http://{host}/car/modify",
                "mqTo": "/webapi/request"
            },
            {
                "objectType": "RemoveCar",
                "httpTo": "http://{host}/car/remove",
                "mqTo": "/webapi/request"
            }
        ],
        "subscription": [
            {
                "objectType": "OnRouteUpdate",
                "httpTo": "http://{host}/event/on_route_update",
                "mqTo": "/weapi/event"
            }
        ]
    },
    "router": {
        "httpFrom": "http://{host}",
        "mqFrom": "/router",
        "local": {
            "serviceName": "local_router",
            "priority": 0,
            "httpHelth": "http://{host}/helth",
            "mqHelth": "/webapi/helth",
            "command": [
                {
                    "objectType": "GetRoutes",
                    "httpTo": "http://{host}/routes",
                    "mqTo": "/router/request"
                },
                {
                    "objectType": "GetRoute",
                    "httpTo": "http://{host}/route/get",
                    "mqTo": "/router/request"
                },
                {
                    "objectType": "AddRoute",
                    "httpTo": "http://{host}/route/add",
                    "mqTo": "/router/request"
                },
                {
                    "objectType": "RemoveRoute",
                    "httpTo": "http://{host}/route/remove",
                    "mqTo": "/router/request"
                },
                {
                    "objectType": "GetRouteCommands",
                    "httpTo": "http://{host}/route/commands",
                    "mqTo": "/router/request"
                },
                {
                    "objectType": "GetRouteCommand",
                    "httpTo": "http://{host}/route/commands/get",
                    "mqTo": "/router/request"
                },
                {
                    "objectType": "GetRouteSubscriptions",
                    "httpTo": "http://{host}/route/subscriptions",
                    "mqTo": "/router/request"
                },
                {
                    "objectType": "GetRouteSubscription",
                    "httpTo": "http://{host}/route/subscription/get",
                    "mqTo": "/router/request"
                }
            ],
            "subscription": [
                {
                    "objectType": "OnServiceUnavailable",
                    "httpTo": "http://{host}/router/event/on_service_unavailable",
                    "mqTo": "/router/event"
                }
            ]
        }
    }
}